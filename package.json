{
  "name": "@frankmoney/frank-back",
  "version": "1.0.0",
  "main": "app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "gen": "node scripts gen",
    "lint": "node node_modules/tslint/bin/tslint -p . && echo \"eslint: no lint errors\"",
    "lint-fix": "node node_modules/tslint/bin/tslint -p . --fix && echo \"eslint: no lint errors\"",
    "prettier": "node node_modules/prettier/bin-prettier.js -l **/*.js **/*.ts **/*.json",
    "prettier-fix": "node node_modules/prettier/bin-prettier.js --write **/*.js **/*.ts **/*.json",
    "start": "node scripts start"
  },
  "lint-staged": {
    "*.js": [
      "node scripts pre-commit"
    ],
    "*.ts": [
      "node scripts pre-commit"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "node node_modules/lint-staged"
    }
  },
  "dependencies": {
    "@types/debug": "^0.0.30",
    "apollo-server": "^2.0.0",
    "debug": "^3.1.0",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.0.5",
    "prisma-binding": "^2.1.1",
    "ramda": "^0.25.0",
    "ts-node": "^6.2.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cross-spawn": "^6.0.5",
    "env-cmd": "^8.0.2",
    "eslint": "^5.2.0",
    "eslint-config-frank": "^0.0.6",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "graphql-cli": "^2.16.5",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "1.14.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0"
  }
}
