{
  "name": "@frankmoney/frank-back",
  "version": "1.0.0",
  "main": "app",
  "license": "MIT",
  "private": true,
  "scripts": {
    "gen": "node scripts gen",
    "lint": "eslint . --ext js,jsx,jss --cache && echo \"eslint: no lint errors\"",
    "lint-fix": "find ./src -name \"*.js[xs]\" | xargs eslint --fix",
    "prettier": "node node_modules/prettier/bin-prettier.js -l **/*.js **/*.ts **/*.json",
    "prettier-fix": "node node_modules/prettier/bin-prettier.js --write **/*.js **/*.ts **/*.json",
    "start": "node scripts start"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "node node_modules/lint-staged"
    }
  },
  "dependencies": {
    "apollo-server": "^2.0.0",
    "eslint-config-frank": "^0.0.6",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "esm": "^3.0.72",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-tools": "^3.0.5",
    "prettier": "1.14.0",
    "prisma-binding": "^2.1.1",
    "ramda": "^0.25.0",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.5.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cross-spawn": "^6.0.5",
    "env-cmd": "^8.0.2",
    "eslint": "^5.2.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.77.0",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0"
  }
}
